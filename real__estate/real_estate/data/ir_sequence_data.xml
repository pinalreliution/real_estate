<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Sequences for real.estate -->
        <record id="seq_real_estate" model="ir.sequence">
            <field name="name">Real Estate</field>
            <field name="code">real.estate</field>
            <field name="prefix">RE</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for property.types -->
        <record id="seq_property_types" model="ir.sequence">
            <field name="name">Property Types</field>
            <field name="code">property.types</field>
            <field name="prefix">PT</field>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
